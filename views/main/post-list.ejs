    <div class="search-page">
      
      <div class="jobs-search-page-content" id="main-content">
      
          <div class="jobs-search-page-content-left">
            <% if (publisheds.length <= 0) { %> 
              <h1>Nothing there!</h1>
              <% } else { %> 
                <% publisheds.forEach(published =>  { %>

          
                  <% published.jobs.forEach(job => {%> 
            <div class="jobs-search-container">
            

              <a href="/admin/job-description/<%= job.id %>?"><%= job.title %>   
                <p>Company Name</p>
              <p><%= job.title %> </p>
              <p><%= job.contract %></p>
              <p><%= job.description %></p>
              <p><%= job.id %></p>
            </a>
            </div>
            <% }) %>
            <% }) %>
            <% } %>
        
       
           
          
